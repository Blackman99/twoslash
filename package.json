{
  "name": "twoslashes",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "author": "TypeScript team",
  "description": "Rewrite of TypeScript TwoSlash",
  "homepage": "https://github.com/antfu/twoslashes",
  "repository": {
    "url": "https://github.com/antfu/twoslashes",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/antfu/twoslashes/issues"
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./core": {
      "types": "./dist/core.d.mts",
      "import": "./dist/core.mjs"
    }
  },
  "scripts": {
    "bootstrap": "pnpm run build",
    "typecheck": "tsc --noEmit",
    "build": "unbuild",
    "test": "jest",
    "lint": ""
  },
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "@types/lz-string": "^1.5.0",
    "@types/prettier": "^1.19.0",
    "@types/react": "^18.2.47",
    "jest": "^29.5.0",
    "jest-file-snapshot": "^0.3.8",
    "lz-string": "^1.5.0",
    "prettier": "^2.3.2",
    "react": "^18.2.0",
    "ts-jest": "^29.0.5",
    "tslib": "^1.10.0",
    "typescript": "^5.3",
    "unbuild": "^2.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "watchPathIgnorePatterns": [
      "test/results"
    ]
  },
  "browser": {
    "fs": false,
    "typescript": false
  },
  "dependencies": {
    "@typescript/vfs": "1.5.0"
  },
  "peerDependencies": {
    "typescript": "*"
  }
}
